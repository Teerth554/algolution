//	Write a program that takes 5 integers as input
//from the user and print Max of all the numbers
#include <stdio.h>

int main()
{
    int a[10];

    for(int i=0;i<=4;i++){
        printf("enter %d value",i+1);
        scanf("%d",&a[i]);
        }
    printf(" you have entered");
    for(int i=0;i<=4;i++){
        printf("%d ",a[i]);
    } 
    
    for(int i=0;i<=4;i++){
        if(a[0]<a[i])
        a[0]=a[i];
    }
    printf("max value is %d",a[0]);
    return 0;
}
