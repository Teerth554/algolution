
#include <iostream>
using namespace std;
int main()
{
    for(int i=0;i<5;i++)
    {
        for(int j=0;j<5;j++)
        {
            if(i+j>=4)
            cout<<"5";
            else if(i+j==3)
            cout<<"4";
            else if(i+j==2)
            cout<<"3";
            else if(i+j==1)
            cout<<"2";
            else cout<<"1";
        }
        cout<<endl;
    }

    return 0;
}
