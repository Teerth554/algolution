//4.	Write a program that takes 7 integers as input from the user 
//then Reverse the order of numbers in the array, then print the numbers.

#include <iostream>
using namespace std;
int main()
{
    int a[7];
    for(int i=0;i<6;i++)
    {
        cout<<"enter "<<i+1<<"value "<<endl;
        cin>>a[i];
        
    }
    
  for(int i=6;i>0;i--)
    {
        cout<<a[i]<<" ";
        
    }

    return 0;
}
