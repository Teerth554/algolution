

#include <iostream>
using namespace std;
int main()
{
    int a[6]={3,2,1,5,6,4};
    int k=4;
    int v=0;
    int max;
    while(k--)
    {
        max=a[0];
     for(int i=0;i<6;i++)
     {
        if(a[i]>max)
        {
            max=a[i];
            v=i;
        }
     }
     a[v]=-1;
    }
    cout<<max;
    return 0;
    
}
