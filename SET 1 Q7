#include <algorithm> // For std::sort
#include <iostream>
using namespace std;

int main() {
    const int size = 12;
    int arr[size] = {1, 1, 3, 3, 3, 10, 1, 3, 3, 7, 7, 8};
    sort(arr, arr + size);

    int uniqueArr[size];
    int uniqueSize = 0;

    for (int i = 0; i < size; ++i) {
        if (i == 0 || arr[i] != arr[i - 1]) {
            uniqueArr[uniqueSize++] = arr[i];
        }
    }

    for (int i = 0; i < uniqueSize; ++i) {
        cout << uniqueArr[i] << " ";
    }
    cout << endl;

    return 0;
}
