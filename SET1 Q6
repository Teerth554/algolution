#include <iostream>
using namespace std;


int main() {
    int size, target;

    cout << "Enter the size of the array: ";
    cin >> size;
    
    int arr[size];

    cout << "Enter the elements of the array:" << endl;
    for (int i = 0; i < size; i++) {
        cin >> arr[i];
    }


    cout << "Enter  number to search: ";
    cin >> target;

     for (int i = 0; i < size; i++) {
        if (arr[i] == target) {
            
            cout<<i;
            break;
        }
    }
    cout<<-1;
    return 0;
}
