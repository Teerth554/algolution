
#include <iostream>
using namespace std;

bool palinn(const string& z) {
    int n = z.length();
    for (int i = 0; i < n / 2; i++) {
        if (z[i] != z[n - 1 - i]) 
            return false;
    }
    return true;
}
int main()
{
    string a="AaaA";
    string z="";
    
    for(int i=0;i<a.size();i++)
    {
        a[i]=tolower(a[i]);
        if(a[i]>='a' && a[i]<='z')
        {
            z+=a[i];
        }
    }
cout << (palinn(z) ? "true" : "false") << endl;;
    return 0;
}
